{
  "name": "all-nigerian-footballers",
  "version": "1.0.0",
  "description": "An open source project for infos on the past and present players of the super eagles",
  "author": "Danang Eko Alfianto <github.com/danangekal>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "reactjs",
    "typescript",
    "tailwindcss",
    "eslint",
    "prettier",
    "lint-staged",
    "pwa",
    "seo"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(js|ts|tsx)\" --quiet --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(js|ts|tsx|css|scss|json|md|html)\" --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
